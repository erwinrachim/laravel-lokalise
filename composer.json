{
  "name": "erwinrachim/laravel-lokalise",
  "description": "Upload or Download your laravel language to or from https://lokalise.com automatically by using laravel commands. fork from najibismail/laravel-lokalise",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "laravel",
    "lokalise"
  ],
  "authors": [
    {
      "name": "Mohd Najib Ismail",
      "email": "najibismail1986@gmail.com"
    },
    {
      "name": "Erwin Abdurachim",
      "email": "erwin.rachim@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
    "lokalise/php-lokalise-api": "^4.0",
    "spatie/emoji": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Erwinrachim\\LaravelLokalise\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Erwinrachim\\LaravelLokalise\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Erwinrachim\\LaravelLokalise\\LaravelLokaliseServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0"
  },
  "prefer-stable": true
}
