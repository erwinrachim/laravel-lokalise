{
    "name": "najibismail/laravel-lokalise",
    "description": "Upload or Download your laravel language to or from https://lokalise.com automatically by using laravel commands.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "lokalise"
    ],
    "authors": [
        {
            "name": "Mohd Najib Ismail",
            "email": "najibismail1986@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "lokalise/php-lokalise-api": "^4.0",
        "spatie/emoji": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Najibismail\\LaravelLokalise\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Najibismail\\LaravelLokalise\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Najibismail\\LaravelLokalise\\LaravelLokaliseServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "prefer-stable": true
}
